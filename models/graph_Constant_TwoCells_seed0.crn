  aCFP = 26720.243552605294, {interval=Log; distribution=TruncatedNormal(121320,227668.9556131885,100,1000000); variation=Random};
  aYFP = 21208.183132451064, {interval=Log; distribution=TruncatedNormal(497900,162909.7602969202,100,1000000); variation=Random};
