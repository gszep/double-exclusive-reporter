  KS6 = 1.0191999416789388E-08, {interval=Log; distribution=TruncatedNormal(3.495600000000001E-05,0.00014254642922220114,1E-08,1); variation=Random};
  KR12 = 1.0123635855026776E-08, {interval=Log; distribution=TruncatedNormal(0.0015554711500000006,0.006797169274797794,1E-08,1); variation=Random};
  KS12 = 0.004073162129419886, {interval=Log; distribution=TruncatedNormal(0.026280033949999997,0.006131803618063334,1E-08,1); variation=Random};
  nR = 0.7649730930420996, {interval=Real; distribution=TruncatedNormal(0.86475,0.20418058796075564,0.5,2); variation=Random};
  nS = 0.7044502919153653, {interval=Real; distribution=TruncatedNormal(1.62525,0.20434049402895862,0.5,2); variation=Random};
  nL = 0.6306204678338496, {interval=Real; distribution=Uniform(0.5,4); variation=Random};
  nT = 1.8183293659014936, {interval=Real; distribution=Uniform(0.5,4); variation=Random};
  aR33 = 9.814331555227724, {interval=Log; distribution=TruncatedNormal(16.776999999999997,27.01577094494991,1,100); variation=Random};
